{
    "extends": [
        "airbnb-base",
        "plugin:vue/recommended"
    ],
    "env": {
        "browser": true
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "object-curly-newline": "off",
        "no-param-reassign": "off",
        "no-lonely-if": "off",
        "import/no-named-as-default": "off",
        "import/no-named-as-default-member": "off"
    }
}
